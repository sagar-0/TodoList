<%-include("header");-%>
    <div class="box" id="heading">
        <h1>  <%= day %> </h1>
    </div>
    <div class="box">
      <%for(var i=0;i<newItem.length;i++){%>
      <div class="item">

        <input type="checkbox">

        <p><%=newItem[i]%></p>
        </div>
        <%}%>



    <form class="item" action="addTodo" method="post">
        <input type="text" name="addItem" placeholder="Enter here" autocomplete="off">
        <button type="submit" name="Add" value=<%= day %>>+</button>
    </form>

</div>
<%-include("footer");-%>
